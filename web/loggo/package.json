{
  "name": "loggo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "proto-gen": "protoc --plugin=protoc-gen-ts=$(which protoc-gen-ts) --js_out=import_style=commonjs,binary:./src/proto --ts_out=service=grpc-web:./src/proto -I ../../protobuf ../../protobuf/loggo/*.proto ../../protobuf/google/api/*.proto",
    "build-themes": "devextreme build",
    "postinstall": "npm run build-themes"
  },
  "private": true,
